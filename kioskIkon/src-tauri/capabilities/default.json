{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "shell:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "exec-cscript",
          "cmd": "cscript",
          "args": [
            "C:\\ikon\\ikon_pos.vbs"
          ]
        }
      ]
    }
  ]
}